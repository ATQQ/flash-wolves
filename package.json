    "build-types": "tsc --emitDeclarationOnly -p ./ && yarn patch-types && yarn roll-types",
    "patch-types": "node scripts/patch-types.js",
    "roll-types": "api-extractor run && rimraf dist/temp"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.18.1",
