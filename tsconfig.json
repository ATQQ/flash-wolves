{
  "compilerOptions": {
    "target": "ES5",
    "module": "CommonJS",
    "lib": ["ESNext"],
    "moduleResolution": "Node",
    "downlevelIteration": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "allowJs": true,
    "experimentalDecorators":true,
    "noImplicitThis": false,
    "noImplicitAny": false,
    "declaration": true,
    "baseUrl": ".",
    "outDir": "./dist",
    "noImplicitOverride": true,
    "noUnusedLocals": false,
  },
  "exclude": [
    "node_modules",
  ]
}